---
generators:
{% for i in ilts %}
  {% set ilt_loop = loop %}
  {# i.start_date, i.end_date, and i.class_session_id #}
  {% for student in range(1, 20) %}
    {% set class_session_id = i.class_session_id + "_student_" + student|string %}
    {# resource_id:  date ilt# student# #}
    {% set resource_id_base = i.start_date|replace("-","") + '%02d'|format(ilt_loop.index) + '%02d'|format(student) %}
# example ILT
# 4 EIPs
# 1 client-vm
# 3 masters
#   1 EBS volume
# 3 workers
#   2 EBS volumes
# 2 S3 buckets
#
# ILT {{ ilt_loop.index }}
  # student {{ student }}
#
# start AWS template
#

  - VPCGenerator:
      start_date: "{{ i.start_date }}"
      end_date: "{{ i.end_date }}"
      tags:
        resourceTags/user:app: cost
        class_session_id: "{{ class_session_id }}"
  - Route53Generator:
      start_date: "{{ i.start_date }}"
      end_date: "{{ i.end_date }}"
      # product_family: DNS Zone
      tags:
        resourceTags/user:app: catalog
        class_session_id: "{{ class_session_id }}"
  - Route53Generator:
      start_date: "{{ i.start_date }}"
      end_date: "{{ i.end_date }}"
      product_family: DNS Query
      tags:
        resourceTags/user:app: cost
        class_session_id: "{{ class_session_id }}"
  - Route53Generator:
      start_date: "{{ i.start_date }}"
      end_date: "{{ i.end_date }}"
