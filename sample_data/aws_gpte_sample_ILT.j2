      product_sku: VEAJHRNKTJZQ
      region: us-east-1a
      tags:
        resourceTags/user:version: "1.0"
        class_session_id: "{{ class_session_id }}"
      instance_type:
        inst_type: t2.medium
        vcpu: 2
        memory: '4 GiB'
        storage: 'EBS Only'
        family: 'General Purpose'
        cost: 0.0464
        rate: 0.0464
