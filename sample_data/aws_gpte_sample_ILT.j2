        resourceTags/user:app: analytics
        class_session_id: "{{ class_session_id }}"
  - S3Generator:
      start_date: "{{ i.start_date }}"
      end_date: "{{ i.end_date }}"
      product_sku: VEAJHRNAAAAA
      amount: 500
      rate: 0.023
